<br/ ><h1>Welcome to <%= @tooder.tooder%>'s Toos</h1><br />

<% @toos.each do |too| %>
	<div class=''>
	  <a href="/<%= too.id %>"><img class = 'too' src='/<%=too.too_file%>'></a>
	  <p><%= too.rated %>%</p>
	</div>
	<!-- make user delete -->
	<form method = 'POST' action = '/tooder/<%=params[:id]%>'>
		<input type="hidden" name="_method" value= 'DELETE'>
		<input type="submit" name="Delete" value='Remove'>
	</form>


<% end %>